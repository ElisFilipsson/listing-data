{
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "prettier"
    ],
    "env": {
      "node": true
    },
    "extends": ["airbnb","prettier"],
    "rules": {
      "quotes": 0,
      "new-cap": 0,
      "consistent-return": 0,
      "global-require": 0,
      "arrow-body-style": 0,
      "prefer-template": 0,
      "no-else-return": 0,
      "no-use-before-define": 0,
      "react/jsx-space-before-closing": 0,
      "import/prefer-default-export": 0,
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "semi":0,
      "yoda": 1,
      "comma-dangle": 0,
      "prettier/prettier": "error"
    }
  }